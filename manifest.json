{
  "name": "traveller's dialy",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "icons": {
    "16" : "images/sampleicon.png",
    "48" : "images/sampleicon.png"
  },
  "background": {
    "scripts":["src/background.js"],
    "persistent": true
  },
  "browser_action": {},
  "permissions": [
    "tabs"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/sampleicon.png",
    "default_title": "mewmew"
  }
}
